<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.seance" table="seance">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
		<property name="date_debut" type="java.lang.String">
			<column name="date_debut" />
		</property>
		<property name="date_fin" type="java.lang.String">
			<column name="date_fin" />
		</property>
		<property name="prof" type="java.lang.String">
			<column name="prof" />
		</property>
		<property name="salle" type="java.lang.String">
			<column name="salle" />
		</property>
		<property name="matiere" type="java.lang.String">
			<column name="matiere" />
		</property>
		<property name="classe" type="java.lang.String">
			<column name="classe" />
		</property>
		<property name="jours" type="java.lang.String">
			<column name="jours" />
		</property>
		<property name="etat" type="java.lang.String">
			<column name="etat" />
		</property>
		
</class>

	
</hibernate-mapping>