<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.classe" table="Classe">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
		<property name="class_nom" type="java.lang.String">
			<column name="class_nom" />
		</property>
		<property name="filiere" type="java.lang.String">
			<column name="filiere" />
		</property>
		<property name="etat" type="java.lang.String">
			<column name="etat" />
		</property>
		
		<property name="departement" type="java.lang.String">
			<column name="departement" />
		</property>
	</class>

	
</hibernate-mapping>