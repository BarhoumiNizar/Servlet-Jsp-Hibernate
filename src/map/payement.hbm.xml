<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.Payement" table="Payement">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
		<property name="py_tranche" type="java.lang.String">
			<column name="py_tranche" />
		</property>
		<property name="py_date" type="java.lang.String">
			<column name="py_date" />
		</property>
		<property name="et_matricule" type="java.lang.String">
			<column name="et_matricule" />
		</property>
		<property name="etat" type="java.lang.String">
			<column name="etat" />
		</property>
		
</class>

	
</hibernate-mapping>