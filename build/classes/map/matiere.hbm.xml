<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.matiere" table="matiere">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
		<property name="mat_nom" type="java.lang.String">
			<column name="mat_nom" />
		</property>
		<property name="mat_mass_horaire" type="java.lang.Integer">
			<column name="mat_mass_horaire" />
		</property>
		<property name="classe" type="java.lang.String">
			<column name="classe" />
		</property>
		<property name="mat_coefficient" type="java.lang.Integer">
			<column name="mat_coefficient" />
		</property>
		<property name="etat" type="java.lang.String">
			<column name="etat" />
		</property>
		<property name="departement" type="java.lang.String">
			<column name="departement" />
		</property>
</class>

	
</hibernate-mapping>